<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="AppropriateCosmeticsPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.appropicatecosmetic.entity.TblUser</class>
    <class>com.appropicatecosmetic.entity.TblSkinType</class>
    <class>com.appropicatecosmetic.entity.TblConcern</class>
    <class>com.appropicatecosmetic.entity.TblCategory</class>
    <class>com.appropicatecosmetic.entity.TblRecommand</class>
    <class>com.appropicatecosmetic.entity.TblProduct</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:sqlserver://localhost:1433;databaseName=AppropicateCosmetic"/>
      <property name="javax.persistence.jdbc.user" value="sa"/>
      <property name="javax.persistence.jdbc.driver" value="com.microsoft.sqlserver.jdbc.SQLServerDriver"/>
      <property name="javax.persistence.jdbc.password" value="12345678"/>
    </properties>
  </persistence-unit>
</persistence>
